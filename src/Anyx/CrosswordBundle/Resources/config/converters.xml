<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
		
    <parameters>
        <parameter key="sensio_framework_extra.converter.doctrine-odm.class">Anyx\CrosswordBundle\Request\ParamConverter\DoctrineMongoDBParamConverter</parameter>
    </parameters>

    <services>
        <service id="sensio_framework_extra.converter.doctrine.odm" class="%sensio_framework_extra.converter.doctrine-odm.class%">
            <tag name="request.param_converter" />
		    <argument type="service" id="doctrine.odm.mongodb.document_manager" />
        </service>
    </services>
